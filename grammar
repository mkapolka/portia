THINGTYPE <- "composite" | "external"
THINGDEF <- THINGTYPE name THINGCONTENTS
THINGCONTENTS <- '{' THINGLINE* '}'
THINGLINE <- PARTDEF | INPUTDEF | OUTPUTDEF
THINGINSTANCE <- name "{" PARTDEF* "}"
PARTDEF <- name '=' PARTVALUE
NUMBER <- '-'? [0-9]+
QUOTE <- '"' | "'"
STRING <- QUOTE .* QUOTE
PARTVALUE <- NUMBER | STRING | THINGINSTANCE
INPUTDEF <- 'input' name ';'
OUTPUTDEF <- 'output' name ';'
